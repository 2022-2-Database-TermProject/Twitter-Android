<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/include"
        layout="@layout/toolbar_logincancel" />

    <TextView
        android:id="@+id/textView4"
        style="@style/tv_b_16"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:text="트위터에서 무엇을 보길 기대하세요?"
        android:textColor="@color/black"
        android:textSize="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        style="@style/tv_16"
        android:textColor="@color/deep_gray"
        android:text="관심사는 내 환경을 맞춤 설정하는 데 활용되며 내 프로필에 표시됩니다."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.03" />

    <View
        android:id="@+id/line"
        android:layout_width="360dp"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.03"
        android:background="@color/light_gray"
        android:visibility="invisible"/>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/finishbutton"
        android:layout_width="70dp"
        android:layout_height="48dp"
        android:background="@drawable/bg_black_circle"
        android:text="완료"
        style="@style/tv_16"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.95" />


    <CheckBox
        android:id="@+id/cb_select_sports"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_check_interest"
        android:button="@null"
        android:checked="false"
        android:enabled="true"
        style="@style/tv_b_16"
        android:gravity="center_horizontal"
        android:text="sports"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:paddingTop="13dp"
        app:layout_constraintBottom_toTopOf="@+id/finishbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.125" />

    <CheckBox
        android:id="@+id/cb_select_game"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_check_interest"
        android:button="@null"
        android:checked="false"
        style="@style/tv_b_16"
        android:enabled="true"
        android:gravity="center_horizontal"
        android:text="game"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:paddingTop="13dp"
        app:layout_constraintBottom_toTopOf="@+id/finishbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.325" />

    <CheckBox
        android:id="@+id/cb_select_study"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_check_interest"
        android:button="@null"
        android:checked="false"
        style="@style/tv_b_16"
        android:enabled="true"
        android:gravity="center_horizontal"
        android:text="study"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:paddingTop="13dp"
        app:layout_constraintBottom_toTopOf="@+id/finishbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.525" />

    <CheckBox
        android:id="@+id/cb_select_entertainment"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_check_interest"
        android:button="@null"
        android:checked="false"
        android:enabled="true"
        style="@style/tv_b_16"
        android:gravity="center_horizontal"
        android:text="entertainment"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:paddingTop="13dp"
        app:layout_constraintBottom_toTopOf="@+id/finishbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.725" />

    <CheckBox
        android:id="@+id/cb_select_food"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_check_interest"
        android:button="@null"
        android:checked="false"
        android:enabled="true"
        android:gravity="center_horizontal"
        android:text="food"
        style="@style/tv_b_16"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:paddingTop="13dp"
        app:layout_constraintBottom_toTopOf="@+id/finishbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.925" />


</androidx.constraintlayout.widget.ConstraintLayout>